QWidget#ParserBg{
    border-image: url(skin/DustyBlue/MainUI/login/bg.png);
}

QLabel#msgLabel{
	background-color:rgb(188,63,63);
	padding-left:40px;
	padding-right:40px;
}

QPushButton#btnQuit{
    outline: none;
    background-color: transparent;
    background-position: center;
    background-repeat: none;
	border-image: url(skin/DustyBlue/MainUI/login/winClose-n.png);
}
QPushButton#btnQuit:hover{
	border-image: url(skin/DustyBlue/MainUI/login/winClose-h.png);
}
QPushButton#btnQuit:pressed{
	border-image: url(skin/DustyBlue/MainUI/login/winClose-p.png);
}
QPushButton#btnQuit:disabled{
	border-image: url(skin/DustyBlue/MainUI/login/winClose-d.png);
}

QPushButton#startRecordBtn{	
    outline: none;
    background-position: center;
    background-repeat: none;
	border-image: url(skin/DustyBlue/MainUI/login/btn-n.png);
}
QPushButton#startRecordBtn:hover{
	border-image: url(skin/DustyBlue/MainUI/login/btn-h.png);
}
QPushButton#startRecordBtn:pressed{
	border-image: url(skin/DustyBlue/MainUI/login/btn-p.png);
}
QPushButton#startRecordBtn:disabled{
	border-image: url(skin/DustyBlue/MainUI/login/btn-d.png);
}

QPushButton#endRecordBtn{	
    outline: none;
    background-position: center;
    background-repeat: none;
	border-image: url(skin/DustyBlue/MainUI/login/btn-n.png);
}
QPushButton#endRecordBtn:hover{
	border-image: url(skin/DustyBlue/MainUI/login/btn-h.png);
}
QPushButton#endRecordBtn:pressed{
	border-image: url(skin/DustyBlue/MainUI/login/btn-p.png);
}
QPushButton#endRecordBtn:disabled{
	border-image: url(skin/DustyBlue/MainUI/login/btn-d.png);
}


DCM--NoticeFrame#LAB_USER[notice=false]{
	border-image: url(skin/DustyBlue/MainUI/login/user-n.png);
}
DCM--NoticeFrame#LAB_USER[notice=true]{
	border-image: url(skin/DustyBlue/MainUI/login/user-e.png);
}
DCM--NoticeFrame#LAB_PASSWORD[notice=false]{
	border-image:url(skin/DustyBlue/MainUI/login/password-n.png);
}
DCM--NoticeFrame#LAB_PASSWORD[notice=true]{
	border-image:url(skin/DustyBlue/MainUI/login/password-e.png);
}

DCM--NoticeFrame#LAB_ADDRESS{
	border-image:url(skin/DustyBlue/MainUI/login/address-n.png);
}
DCM--NoticeFrame#LAB_PORT{
	border-image:url(skin/DustyBlue/MainUI/login/port-n.png);
}


